{
  "name": "reactjs-feature-flags",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@paralleldrive/react-feature-toggles": "^2.3.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "axios": "^1.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-server": "minikube start && eval $(minikube docker-env) && docker build -f Dockerfile.server -t ft_poc:1.0.0 . && kubectl apply -f deployments/configMaps.yaml && kubectl apply -f deployments/deployments.yaml",
    "build-web": "docker build -f Dockerfile -t react-js-feature:1.0.0-prod . ",
    "start-server": "minikube service ft",
    "start-web": "minikube service minikube-reactjs-feature-flags",
    "apply-config-maps": "kubectl apply -f deployments/configMaps.yaml",
    "delete-pods": "minikube pause -A && minikube delete --all",
    "delete-images": "docker rmi $(docker images -q)"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
